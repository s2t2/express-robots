<!DOCTYPE html>
<html>
  <head>
    <% include ../partials/head %>
  </head>
  <body>
    <header>
      <% include ../partials/header %>
    </header>

    <h2><%= page_title %></h2>

    <table style="width:100%">
      <tr>
        <td>Robot Id</td>
        <td>Robot Name</td>
      </tr>
      <% robots.forEach(function(robot){ %>
        <tr>
          <td><%= robot.id %></td>
          <td><%= robot.name %></td>
        </tr>
      <% }); %>
    </table>



    <hr style="margin-top:2em;">
    <footer>
      <% include ../partials/footer %>
    </footer>
  </body>
</html>
